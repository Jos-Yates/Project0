package models;

public class Customer {
	private Integer id;
	private String name;
	private String username;
	private String password;
	private Integer account_balance;
	
	
	public Customer() {
		super();
	}
	
	public Customer(String name, String username, String password, Integer account_balance) {
		super();
		this.name = name;
		this.username = username;
		this.password = password;
		this.account_balance = account_balance;
	}
	
	public Customer(Integer id, String name, String username, String password, Integer account_balance) {
		super();
		this.id = id;
		this.name = name;
		this.username = username;
		this.password = password;
		this.account_balance = account_balance;
	}

	
}
